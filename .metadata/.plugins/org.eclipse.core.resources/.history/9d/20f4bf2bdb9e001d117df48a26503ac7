package com.masai;

public class SecondSmallAnd2ndLargest {

	public static void main(String[] args) {
		int[] arr = {1,2,4,7,7,5};
		int sl=arr[0], l=arr[0], ss=arr[0], s=arr[0];
		
		for(int i=0; i<arr.length; i++) {
			if(arr[i] > l) {
				sl = l;
				l = arr[i];
			}
		}
		
		System.out.println(sl);
	}

}
