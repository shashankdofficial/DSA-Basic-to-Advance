package com.masai;

public class FindMedian {

	public static void main(String[] args) {
		
		int[] arr = {2,4,1,3,5};
		int n = arr.length;
		
		if(n%2 == 0) {
			double a = Math.floor((n/2));
			double b = Math.ceil((n/2));
			
			System.out.println(arr[a]+arr[b]/2);
		}
		else {
			System.out.println(arr[n/2]);
		}
	}

}
